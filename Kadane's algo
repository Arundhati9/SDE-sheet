class Solution {
public:
    int maxSubArray(vector<int>& nums) {
        
        int maxsum = INT_MIN;
        int sum = 0;
        for(auto it: nums)
        {
            sum+= it;
            if(sum>maxsum)
                maxsum = sum;
            if(sum<0)
                sum = 0;
        }
        return maxsum;
    }
};
